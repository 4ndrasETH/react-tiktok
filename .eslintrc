{
    "extends": ["react-app"],
    /** These are optional: */
    "rules": {
      /** Warn if console.log are added */
      "no-console": "warn",
  
      /** Enforcing rules about import ordering */
      "import/order": [
        "warn",
        {
          "groups": [["builtin", "external"], "internal", ["parent", "sibling", "index"]],
          "newlines-between": "always-and-inside-groups"
        }
      ]
    }
  }
  